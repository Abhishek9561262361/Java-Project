<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="com.jspiders.spring_core.beans.PhoneBean"
		name="doctoe ">
		<property name="id" value="1" />
		<property name="name" value="Samsung" />
		<property name="price" value="30000" />
		<property name="applications">
			<list>
				<bean class="com.jspiders.spring_core.beans.ApplicationBean"
					name="App1">
					<property name="id" value="1" />
					<property name="name" value="Instagram" />
					<property name="size" value="120.2" />
				</bean>
				<bean class="com.jspiders.spring_core.beans.ApplicationBean"
					name="App2">
					<property name="id" value="2" />
					<property name="name" value="Telegram" />
					<property name="size" value="60.2" />
				</bean>
			</list>
		</property>
	</bean>
	<bean class="com.jspiders.spring_core.beans.PhoneBean"
		name="phone2">
		<property name="id" value="2" />
		<property name="name" value="Realme" />
		<property name="price" value="20000" />
		<property name="applications">
			<list>
				<bean class="com.jspiders.spring_core.beans.ApplicationBean"
					name="App1">
					<property name="id" value="1" />
					<property name="name" value="Instagram" />
					<property name="size" value="120.2" />
				</bean>
				<bean class="com.jspiders.spring_core.beans.ApplicationBean"
					name="App2">
					<property name="id" value="2" />
					<property name="name" value="Telegram" />
					<property name="size" value="60.2" />
				</bean>
			</list>
		</property>
	</bean>
</beans>